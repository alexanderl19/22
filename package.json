{
	"name": "22",
	"private": true,
	"version": "0.0.1",
	"type": "module",
	"scripts": {
		"dev": "dotenvx run -- vite dev",
		"build": "vite build",
		"cf-typegen": "wrangler types ./src/worker-configuration.d.ts",
		"prepare": "svelte-kit sync || echo ''",
		"check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --write .",
		"lint": "prettier --check . && eslint .",
		"db:push": "drizzle-kit push",
		"db:generate": "drizzle-kit generate",
		"db:migrate": "drizzle-kit migrate",
		"db:studio": "drizzle-kit studio",
		"preview": "pnpm run build && wrangler dev"
	},
	"devDependencies": {
		"@dotenvx/dotenvx": "1.51.1",
		"@electric-sql/client": "1.0.14",
		"@eslint/compat": "1.4.0",
		"@eslint/js": "9.37.0",
		"@fontsource-variable/archivo": "^5.2.8",
		"@fontsource/archivo": "^5.2.8",
		"@fontsource/commit-mono": "^5.2.5",
		"@js-draw/math": "1.31.1",
		"@sveltejs/adapter-cloudflare": "7.2.4",
		"@sveltejs/enhanced-img": "0.8.3",
		"@sveltejs/kit": "2.43.8",
		"@sveltejs/vite-plugin-svelte": "6.2.1",
		"@tanstack/electric-db-collection": "0.1.32",
		"@tanstack/svelte-db": "0.1.30",
		"@types/node": "22.18.8",
		"cli-spinners": "3.3.0",
		"clsx": "2.1.1",
		"debounce-fn": "6.0.0",
		"drizzle-kit": "0.31.5",
		"drizzle-orm": "0.44.6",
		"eslint": "9.37.0",
		"eslint-config-prettier": "10.1.8",
		"eslint-plugin-svelte": "3.12.4",
		"globals": "16.4.0",
		"isomorphic-dompurify": "2.29.0",
		"js-draw": "1.31.1",
		"lucide-svelte": "0.545.0",
		"postgres": "3.4.7",
		"prettier": "3.6.2",
